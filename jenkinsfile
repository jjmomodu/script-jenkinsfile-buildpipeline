    agent any
	
    stages {

       stage("Build") {
		
	   steps {
	       echo "Building The Application..."
	   }
	}

        stage("Test") {
		
	   steps {
	       echo "Testing The Application..."
	   }
	}

        stage("Deploy") {
		
	   steps {
	       echo "Deploying The Application..."
	   }
	}
	 stage("Release") {
		
	   steps {
	       echo "Releaseing The Application..."
	   }
	}
    }
}
